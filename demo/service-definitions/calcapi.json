{
  "swaggerVersion": "1.2",
  "basePath": "http://calcapi.cloudapp.net/api",
  "apis": [
    {
      "path": "add?a={a}&b={b}",
      "description": "Integer addition.",
      "operations": [
        {
          "method": "GET",
          "summary": "Produces a sum of two numbers.",
          "integer": "string",
          "nickname": "Add two integers",
          "parameters": [
            {
              "name": "a",
              "description": "First operand. Default value is <code>51</code>.",
              "required": true,
              "type": "integer",
              "paramType": "query",
              "defaultValue": 51
            },
            {
              "name": "b",
              "description": "Second operand. Default value is <code>49</code>.",
              "required": true,
              "type": "integer",
              "paramType": "query",
              "defaultValue": 49
            }
          ]
        }
      ]
    },
